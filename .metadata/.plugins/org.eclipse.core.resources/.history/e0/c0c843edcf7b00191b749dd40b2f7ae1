package com.ego.dubbo.service;

import com.ego.commons.pojo.EasyUIDataGrid;
import com.ego.pojo.TbContent;

public interface TbContentDubboService {
	/**
	 * 分页查询
	 * @param cateId
	 * @param page
	 * @param rows
	 * @return
	 */
	EasyUIDataGrid selContentByPage(long cateId,int page,int rows);
	/**
	 * 新增
	 * @param content
	 * @return
	 */
	int insContent(TbContent content);
	/**
	 * 根据id修改
	 * @param content
	 * @return
	 */
	int updContentById(TbContent content);
	/**
	 * 根据id删除
	 * @param content
	 * @return
	 */
	int delContentById(TbContent content);
}
